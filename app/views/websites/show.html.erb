<p>
  <b>Id:</b>
  <%=h @website.id %>
</p>


<p>
  <b>Assigned Port Numbers:</b>
  <%=h @website.id.to_s + '0*' %>
</p>


<p>
  <b>Name:</b>
  <%=h @website.name %>
</p>

<p>
  <b>Title:</b>
  <%=h @website.title %>
</p>

<p>
  <b>Default item department ids:</b>
  <%=h @website.default_item_department_ids %>
</p>

<p>
  <b>Default design department ids:</b>
  <%=h @website.default_design_department_ids %>
</p>


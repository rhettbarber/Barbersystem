



<h1>Listing custom_items</h1>

<table>
  <tr>
    <th>Image</th>
    <th>IM Filename</th>
  </tr>

<% for custom_item in @custom_items %>

  <tr>
	<td><%= link_to image_tag(custom_item.public_filename, :size => "100x100")  %></td>
	   <td><%=  custom_item.im_filename  %></td>
    <td><%= link_to 'Show', custom_item %></td>
    <td><%= link_to 'Edit', edit_custom_item_path(custom_item) %></td>
    <td><%= link_to 'Destroy', custom_item, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />






<%= link_to 'New custom_item', new_custom_item_path %>




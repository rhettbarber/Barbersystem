<%= ssd %>

<div class="row-fluid">
  <ul class="thumbnails">
    <% @design_departments.each do |department| %>
        <li class="col-6 col-lg-3">
          <a href="/store/browse?department_id=<%= department.id %>" class="thumbnail" data-remote="true">
            <img src="<%= 'http://cdn.dixieoutfitters.com/images/departments/' + department.Name + '.png' %>" alt="260x180">
          </a>
        </li>
    <% end %>
  </ul>
</div>



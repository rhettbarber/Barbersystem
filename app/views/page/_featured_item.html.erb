<div class="carousel_item_container">
  <div class="featured-designs-carousel-image-container">

                                  <% if item.onclick and item.onclick  != "" %>
                                                     <a href="#" onclick="<%= item.onclick %>"><img src="http://cdn.dixieoutfitters.com/<%= item.image_name %>" />  </a>
                                 <% elsif  item.carousel_url  %>
                                                  <a href="<%= item.carousel_url %>"><img src="http://cdn.dixieoutfitters.com/<%= item.image_name %>" />  </a>
                                <% else %>
                                                    <a href="/featured_items/list"><img src="http://cdn.dixieoutfitters.com/<%= item.image_name %>" />  </a>
                               <% end %>
  </div>

  <!--<div class="carousel_link_container_100"><a data-role="button" data-theme="b" href="/store/new_designs">Shop New Designs </a></div>-->
</div>
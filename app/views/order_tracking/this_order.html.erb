<h1>OrderTracking#this_order</h1>
<% if @purchase %>


<p>Your current purchase: <%= @purchase.id %></p>
<p> @purchase.inspect: <%= @purchase.inspect %></p>



<div class="product_container_div">
            	<table id="purchase_table" class="purchase_table">
                  <tr>
                                            <th  class="purchases_entry_delete">.</th>
                                            <th  class="purchases_entry_master_image">Item</th>
											<th  class="purchases_entry_slave_image">Design</th>
                                            <th  class="purchases_entry_quantity">Quantity</th>
                                            <th  class="purchases_entry_itemlookupcode">Item lookup code</th>
                                            <th  class="purchases_entry_symbiont_description">Description</th>
                                            <th  class="purchases_entry_full_unit_price">Unit price:</th>
                                            <th  class="purchases_entry_full_extended_price">Extended price</th>
                  </tr>
                        <% if  @purchase.purchases_entries %>



                                  found @purchase.purchases_entries


			                                            <% @purchase.purchases_entries.each do |purchases_entry|   %>
                                                                                      <%= render(:partial => "order_tracking/shared_display_cart_purchases_entry",:locals => { :purchases_entry => purchases_entry } ) %>

			                                            <% end     %>






					   <% end %>
                        </table>

</div>




<% else %>
You do not have an order in progress.

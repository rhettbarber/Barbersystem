    <th>Website</th>
    <th>Id</th>
    <th>Category</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @hidden_website_categories.each do |hidden_website_category| %>
  <tr>
    <td><%= hidden_website_category.website_id %></td>
    <td><%= hidden_website_category.id %></td>
    <td><%= hidden_website_category.category_id %></td>
    <td><%= link_to 'Show', hidden_website_category %></td>
    <td><%= link_to 'Edit', edit_hidden_website_category_path(hidden_website_category) %></td>
